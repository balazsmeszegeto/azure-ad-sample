@model azuread_sample.LoggedInModel
<html>
<head>
    <meta charset="utf-8"/>
</head>
<body>
    You are logged in as <em>@Model.Email</em>. @Html.ActionLink("Log out", "Logout") <br/>

    <dl>
        @foreach (var claim in Model.Claims)
        {
            <dt>@claim.Type</dt>
            <dd>@claim.Value</dd>
        }
    </dl>
</body>
</html>